                     style="overflow: hidden; outline: none;">
                </div>
            </div>
        </div>
        </div>

        <div class="wrap">
            <!---728x90--->

            <div class="header">
                <div class="headertop_desc">

                    @{ Html.RenderAction("TopMenu", "Home");}
                    <div class="clear"></div>
                </div>
                <div class="header_top">
                    <div class="logo">
                        <a href="/Client/Home"><img src="/Assets/Client/images/logo.jpg"  style="width:180px;
                                                      height: 60px; margin-left: 50px;" alt="" /></a>
                    </div>
                    @{ Html.RenderAction("HeaderCart", "Cart");}
                    <script type="text/javascript">
                        function DropDown(el) {
                            this.dd = el;
                            this.initEvents();
                        }
                        DropDown.prototype = {
                            initEvents: function () {
                                var obj = this;

                                obj.dd.on('click', function (event) {
                                    $(this).toggleClass('active');
                                    event.stopPropagation();
                                });
                            }
                        }

                        $(function () {

                            var dd = new DropDown($('#dd'));

                            $(document).click(function () {
                                // all dropdowns
                                $('.wrapper-dropdown-2').removeClass('active');
                            });

                        });

                    </script>
                    <div class="clear"></div>
                </div>
                <!---728x90--->
                <div class="header_bottom">

                    @{ Html.RenderAction("MainMenu", "Home");}
                    <div class="search_box">
                        <form action="/Client/Product/Search" method="get">
                            <input type="text" id="txtKeyword" name="keyword" placeholder="Gõ từ khoá"><input type="submit" value="">
                        </form>
                    </div>
                    <div class="clear"></div>
                </div>
                @RenderSection("slide", required: false)

            </div>
            <div class="main">
                <!---728x90--->
                @RenderBody()

            </div>
        </div>
        <div class="footer">
            @{ Html.RenderAction("Footer", "Home");}
        </div>
        <script type="text/javascript">
            $(document).ready(function () {
                $().UItoTop({ easingType: 'easeOutQuart' });

            });
        </script>
        <a href="#" id="toTop"><span id="toTopHover"> </span></a>
        <script src="/Assets/Client/js/controller/baseController.js"></script>
        @RenderSection("jsFooter", required: false)

